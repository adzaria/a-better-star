{
  "name": "exalt-grade",
  "version": "1.0.25",
  "description": "Slightly exalt grades while keeping order ( [a, b] -> [a, b] and a < b -> f(a) < f(b) )",
  "main": "lib/cjs/index.js",
  "types": "lib/esm/index.d.ts",
  "module": "lib/esm/index.js",
  "scripts": {
    "build": "tsc --build tsconfig-cjs.json && tsc --build tsconfig-esm.json",
    "test": "mocha --file build/test/test.js",
    "test-ts": "mocha -r ts-node/register --file test/test.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test-ts",
    "preversion": "",
    "version": "npm run format && git add .",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adzaria/exalt-grade.git"
  },
  "keywords": [
    "exalt-grade",
    "grades",
    "exalt",
    "improve",
    "better",
    "stars"
  ],
  "author": "adzaria",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adzaria/exalt-grade/issues"
  },
  "files": ["lib/**/*"],
  "homepage": "https://github.com/adzaria/exalt-grade#readme",
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.1",
    "@types/node": "^14.0.27",
    "chai": "^4.2.0",
    "mocha": "^8.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
